    "framer-motion": "^8.1.5",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "jest": "^29.3.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.20",
    "next": "^13.1.1",
    "nookies": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
